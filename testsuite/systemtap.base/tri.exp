# Simple function to test that systemtap ?: operation works

set test "tri"

foreach runtime [get_runtime_list] {
    if {$runtime != ""} {
	stap_run $srcdir/$subdir/$test.stp no_load $all_pass_string \
	    --runtime=$runtime
    } else {
	stap_run $srcdir/$subdir/$test.stp no_load $all_pass_string
    }
}
