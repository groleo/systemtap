# Simple function to test that systemtap divide by 0 doesn't kill the machine

set test "div0"

set output_string "(.*)division by 0 near(.*)"

foreach runtime [get_runtime_list] {
    if {$runtime != ""} {
	stap_run $srcdir/$subdir/$test.stp no_load $output_string \
	    --runtime=$runtime
    } else {
	stap_run $srcdir/$subdir/$test.stp no_load $output_string

    }
}
