# Check that global variables terminated with a ; work fine

set test "global_vars"

foreach runtime [get_runtime_list] {
    if {$runtime != ""} {
	stap_run $srcdir/$subdir/$test.stp no_load $all_pass_string \
	    --runtime=$runtime
    } else {
	stap_run $srcdir/$subdir/$test.stp no_load $all_pass_string
    }
}
