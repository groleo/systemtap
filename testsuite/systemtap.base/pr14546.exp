set test "pr14546"
if {! [installtest_p]} {
    untested "$test"
    return
}

set test "pr14546 once"
spawn stap $srcdir/$subdir/pr14546.stp "println(backtrace())"
set ko 0
expect {
    -timeout 60
    timeout { fail "$test (timeout)" }
    -re {^ERROR: [^\r\n]*\r\n} { incr ko; exp_continue }
    eof { }
}
if {$ko == 0} then { pass $test } else { fail "$test ($ko)" }
catch { wait } ; catch { close }

